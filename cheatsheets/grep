# Count number of columns - 1 in CSV file
cat file.csv | grep -o , | wc -l  # need to add 1 to have the number of columns

# Match files ending with .ts, .tsx, .js
git diff --name-only | grep "\(.js\|.tsx\|\.ts\)$"

# Grep from list of words
cat .. | grep 'truc\|test'

# Graph Github personal token from .npmrc
cat ~/.npmrc | grep -o "ghp_.*"

# See also:
# https://learnbyexample.github.io/learn_gnugrep_ripgrep/
